{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "scripts/usePrayerInfo.ts",
    "components/Features/Settings/ManualCorrectionModal.tsx",
    "app/(tab)/(stack)/index.tsx"
  ]
}
